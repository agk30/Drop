{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "google",
    "plugin:mocha/recommended",
    "plugin:security-node/recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["security-node", "mocha", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
